set terminal epslatex size 5,4
set output 'plot_ans2_time.tex'
set xlabel '$k$'
set ylabel 'Search time ($s$)'
set xrange [1:15]
set yrange [0:5]
set xtic 1
plot "$ANS2" using 1:2 pointtype 10  dashtype 10 linecolor rgb "blue" with linespoint
