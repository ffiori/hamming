set terminal epslatex size 5,2.5
set output 'plot_ans2_misses.tex'
set xlabel '$k$'
set ylabel 'Percentage of branch misses'
set xrange [1:15]
set yrange [0:30]
set xtic 1
set key top left
plot "$ANS2" using 1:2 title 'ANS2' pointtype 10  dashtype 10 linecolor rgb "blue" with linespoint
