set terminal epslatex size 6,3.5
set output 'plot_single_dna_m32_few.tex'
set xlabel '$k$'
set ylabel 'Search time ($s$)'
set xrange [1:3]
set xtic 1
set yrange [0:2]

set key outside
set key bottom right

plot \
"$EFS" using 1:2    title 'EFS'   pointtype 1 linecolor rgb "black" with linespoint , \
"$TwSA" using 1:2   title 'TwSA'  pointtype 2 linecolor rgb "0x4C0099" with linespoint , \
"$ANS2b" using 1:2  title 'ANS2b' pointtype 3 linecolor rgb "red" with linespoint , \
"$BYPSb" using 1:2  title 'BYPSb' pointtype 4 linecolor rgb "0x009900" with linespoint
